<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GlobalAttrs">
        <attr name="title" format="string" localization="suggested"/>
        <attr name="subtitle" format="string" localization="suggested"/>
    </declare-styleable>


    <declare-styleable name="CarouselView">
        <attr name="title"/>
        <attr name="see_more" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CarouselItemView">
        <attr name="title"/>
        <attr name="subtitle"/>
        <attr name="imageSrc" format="reference" />
        <attr name="description" format="string"/>
    </declare-styleable>


</resources>